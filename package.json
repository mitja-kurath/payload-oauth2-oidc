{
  "name": "payload-oauth2-oidc",
  "version": "1.0.2",
  "description": "Flexible OIDC and OAuth2 integration for Payload CMS v3",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean --minify --external payload",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "payload": "^3.0.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.0",
    "payload": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.3",
    "openid-client": "^6.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mitja-kurath/payload-oauth2.git"
  },
  "keywords": [
    "payload",
    "payload-plugin",
    "oauth2",
    "oidc",
    "authentication"
  ],
  "author": {
    "name": "Mitja Kurath",
    "email": "hi@mitjakurath.ch"
  },
  "license": "MIT"
}
