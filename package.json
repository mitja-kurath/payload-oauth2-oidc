    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --clean"
  },
  "peerDependencies": {
    "openid-client": "^6.0.0",
    "payload": "^3.0.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "payload": "^3.72.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.3"
  }
}
