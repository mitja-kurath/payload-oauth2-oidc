{
  "name": "payload-oauth2-oidc",
  "version": "1.0.3",
  "description": "Flexible OIDC and OAuth2 integration for Payload CMS v3",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean --minify --external payload",
    "prepublishOnly": "pnpm build",
    "test": "vitest run"
  },
  "peerDependencies": {
    "payload": "^3.73.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "payload": "^3.73.0",
    "tsup": "^8.5.1",
    "typescript": "^5.7.3",
    "vite-tsconfig-paths": "^5.1.0",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.3",
    "openid-client": "^6.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mitja-kurath/payload-oauth2-oidc.git"
  },
  "keywords": [
    "payload",
    "payload-plugin",
    "oauth2",
    "oidc",
    "authentication"
  ],
  "author": {
    "name": "Mitja Kurath",
    "email": "hi@mitjakurath.ch"
  },
  "license": "MIT"
}
